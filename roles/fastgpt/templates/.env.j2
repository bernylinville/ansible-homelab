# This file is managed by Ansible. Do not edit manually.

# General
TIMEZONE={{ timezone }}
MEM_LIMIT={{ ragflow_mem_limit }}

DOC_ENGINE={{ ragflow_doc_engine}}

# MySQL
MYSQL_PASSWORD={{ ragflow_mysql_password }}
MYSQL_PORT={{ ragflow_mysql_port }}

# MinIO
MINIO_CONSOLE_PORT={{ ragflow_minio_console_port }}
MINIO_PORT={{ ragflow_minio_port }}
MINIO_USER={{ ragflow_minio_user }}
MINIO_PASSWORD={{ ragflow_minio_password }}

# Redis
REDIS_PORT={{ ragflow_redis_port }}
REDIS_PASSWORD={{ ragflow_redis_password }}

# Elasticsearch
STACK_VERSION={{ ragflow_es_image.split(':')[1] | default('8.11.3') }}
ES_PORT={{ ragflow_es_port }}
ELASTIC_PASSWORD={{ ragflow_es_password }}

# Hugging Face mirror site (optional)
# HF_ENDPOINT=https://hf-mirror.com

# MacOS optimizations (optional)
# MACOS=true
