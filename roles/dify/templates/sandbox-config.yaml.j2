# Dify Sandbox Configuration
# Generated by Ansible for {{ ansible_hostname }}

app:
  port: 8194
  debug: {{ dify_enable_debug | string | title }}
  key: {{ dify_env.SANDBOX_API_KEY }}

max_workers: {{ dify_sandbox_max_workers | default(4) }}
max_requests: {{ dify_sandbox_max_requests | default(50) }}
worker_timeout: {{ dify_sandbox_worker_timeout | default(15) }}
python_path: /usr/local/bin/python3
enable_network: {{ dify_sandbox_enable_network | default(true) | string | lower }}

allowed_syscalls: # 留空表示使用默认 seccomp 配置

proxy:
  socks5: '{{ dify_sandbox_socks5_proxy | default("") }}'
  http: '{{ dify_sandbox_http_proxy | default("") }}'
  https: '{{ dify_sandbox_https_proxy | default("") }}'